<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.elamoreslomasgrande.volley1.Navigation.PortfolioChildren.PortfolioCV">

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingHorizontal="17dp"
        android:layout_marginTop="19dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="15sp"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif"
                android:textStyle="normal"
                android:textColor="#000000"
                android:letterSpacing="0.03"
                android:text="@string/PortfolioCVCategoria"
                android:paddingVertical="6dp"
                android:paddingHorizontal="15dp"
                android:layout_marginRight="15dp"
                android:background="#33999999"/>
            <ImageView
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/circle" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="15sp"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif"
                android:textStyle="normal"
                android:textColor="#000000"
                android:letterSpacing="0.03"
                android:text="@string/PortfolioCVTipos"
                android:paddingVertical="6dp"
                android:paddingHorizontal="15dp"
                android:layout_marginRight="15dp"
                android:background="#33999999"/>
            <ImageView
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/circle"  />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="15sp"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif"
                android:textStyle="normal"
                android:textColor="#000000"
                android:letterSpacing="0.03"
                android:text="@string/PortfolioCVExperiencia"
                android:paddingVertical="6dp"
                android:paddingHorizontal="15dp"
                android:layout_marginRight="15dp"
                android:background="#33999999"/>
            <ImageView
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/circle"  />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="15sp"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif"
                android:textStyle="normal"
                android:textColor="#000000"
                android:letterSpacing="0.03"
                android:text="@string/PortfolioCVEducacion"
                android:paddingVertical="6dp"
                android:paddingHorizontal="15dp"
                android:layout_marginRight="15dp"
                android:background="#33999999"/>
            <ImageView
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/circle" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20dp">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:textSize="15sp"
                android:layout_centerVertical="true"
                android:fontFamily="sans-serif"
                android:textStyle="normal"
                android:textColor="#000000"
                android:letterSpacing="0.03"
                android:text="@string/PortfolioCVIdiomas"
                android:paddingVertical="6dp"
                android:paddingHorizontal="15dp"
                android:layout_marginRight="15dp"
                android:background="#33999999"/>
            <ImageView
                android:layout_width="36dp"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:src="@drawable/circle"  />
        </RelativeLayout>


    </LinearLayout>

</FrameLayout>
